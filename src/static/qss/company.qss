QFrame#menuFrame {
    background-color: #0D47A1;
    min-height: 40px;
}

QFrame#tabFrame {
    background-color: transparent;
}

QLabel#tabLabel {
    font-size: 14px;
    color: #ffffff;
    padding: 8px 16px;
    margin: 0px;
}

QLabel#tabLabel:hover {
    background-color: #1565C0;
}

QMenu#dropdownMenu {
    background-color: #ffffff;
    border: 1px solid #d3d3d3;
    border-radius: 4px;
}

QMenu#dropdownMenu::item {
    font-size: 12px;
    padding: 8px 16px;
    color: #333333;
}

QMenu#dropdownMenu::item:selected {
    background-color: #f0f8ff;
    color: #333333;
}